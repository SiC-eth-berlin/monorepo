{
  "name": "@sic-monorepo/monorepo",
  "version": "0.0.1",
  "private": "true",
  "scripts": {
    "react-app:build": "yarn workspace @sic-monorepo/react-app build --max-old-space-size=12288",
    "react-app:eject": "yarn workspace @sic-monorepo/react-app eject",
    "react-app:start": "yarn workspace @sic-monorepo/react-app start",
    "react-app:test": "yarn workspace @sic-monorepo/react-app test",
    "build": "yarn workspace @sic-monorepo/react-app build --max-old-space-size=12288",
    "prettier": "yarn workspace @sic-monorepo/react-app prettier",
    "chain": "yarn workspace @sic-monorepo/hardhat chain",
    "fork": "yarn workspace @sic-monorepo/hardhat fork",
    "hardhat": "yarn workspace @sic-monorepo/hardhat hardhat",
    "hardhat:node": "yarn workspace @sic-monorepo/hardhat hardhat node",
    "hardhat:test-module:localhost": "yarn workspace @sic-monorepo/hardhat hardhat test-module --network localhost",
    "hardhat:full-deploy-local": "yarn workspace @sic-monorepo/hardhat full-deploy-local",
    "node": "yarn workspace @sic-monorepo/hardhat chain",
    "test": "yarn workspace @sic-monorepo/hardhat test",
    "start": "yarn workspace @sic-monorepo/react-app start",
    "compile": "yarn workspace @sic-monorepo/hardhat compile",
    "deploy": "yarn workspace @sic-monorepo/hardhat deploy",
    "verify": "yarn workspace @sic-monorepo/hardhat verify",
    "watch": "yarn workspace @sic-monorepo/hardhat watch",
    "accounts": "yarn workspace @sic-monorepo/hardhat accounts",
    "balance": "yarn workspace @sic-monorepo/hardhat balance",
    "send": "yarn workspace @sic-monorepo/hardhat send",
    "ipfs": "yarn workspace @sic-monorepo/react-app ipfs",
    "surge": "yarn workspace @sic-monorepo/react-app surge",
    "s3": "yarn workspace @sic-monorepo/react-app s3",
    "ship": "yarn workspace @sic-monorepo/react-app ship",
    "generate": "yarn workspace @sic-monorepo/hardhat generate",
    "account": "yarn workspace @sic-monorepo/hardhat account",
    "mineContractAddress": "cd packages/hardhat && npx hardhat mineContractAddress",
    "wallet": "cd packages/hardhat && npx hardhat wallet",
    "fundedwallet": "cd packages/hardhat && npx hardhat fundedwallet",
    "flatten": "cd packages/hardhat && npx hardhat flatten",
    "clean": "cd packages/hardhat && npx hardhat clean",
    "theme": "yarn workspace @sic-monorepo/react-app theme",
    "watch-theme": "yarn workspace @sic-monorepo/react-app watch",
    "postinstall": "husky install"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/hardhat",
      "**/hardhat/**",
      "**/hardhat-ts",
      "**/hardhat-ts/**"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^7.0.2"
  }
}
